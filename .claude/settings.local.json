{
  "permissions": {
    "allow": [
      "Bash(bun install:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(define-claude-code-hooks:*)",
      "Bash(bun run:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(npm init:*)",
      "Bash(npm install:*)",
      "Bash(npx:*)",
      "Bash(bun add:*)",
      "Bash(../test-claude-code-hooks/node_modules/.bin/define-claude-code-hooks --help)",
      "Bash(PWD=../test-claude-code-hooks ../test-claude-code-hooks/node_modules/.bin/define-claude-code-hooks --project)",
      "Bash(chmod:*)",
      "Bash(node:*)",
      "Bash(rm:*)",
      "Bash(bash:*)"
    ],
    "deny": []
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node -r ts-node/register --no-warnings \"./.claude/hooks/hooks.ts\" __run_hook Stop # __managed_by_define_claude_code_hooks__"
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node -r ts-node/register --no-warnings \"./.claude/hooks/hooks.ts\" __run_hook SubagentStop # __managed_by_define_claude_code_hooks__"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "node -r ts-node/register --no-warnings \"./.claude/hooks/hooks.ts\" __run_hook PostToolUse \".*\" \"0\" # __managed_by_define_claude_code_hooks__"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": ".*",
        "hooks": [
          {
            "type": "command",
            "command": "node -r ts-node/register --no-warnings \"./.claude/hooks/hooks.ts\" __run_hook PreToolUse \".*\" \"0\" # __managed_by_define_claude_code_hooks__"
          }
        ]
      }
    ]
  }
}